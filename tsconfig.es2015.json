{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"declaration": true,
		"outDir": "./lib/es2015",
		"strict": true,
		"downlevelIteration": true,
		"esModuleInterop": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"test"
	]
}
